{
  "name": "chrome-stylize",
  "author": "Akihiro MATOBA",
  "description": "chrome extension for customize CSS and JS.",
  "version": "1.0.0",
  "manifest_version": 3,
  "web_accessible_resources": [],
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://www.instapaper.com/*"],
      "css": ["instapaper.com/style.css"],
      "js": ["instapaper.com/script.js"]
    },
    {
      "matches": ["https://*.goguynet.jp/*"],
      "css": ["goguynet.jp/style.css"],
      "js": ["goguynet.jp/script.js"]
    },
    {
      "matches": ["https://x.com/*"],
      "css": ["x.com/style.css"],
      "js": ["x.com/script.js"]
    },
    {
      "matches": ["https://togetter.com/*"],
      "css": ["togetter.com/style.css"],
      "js": ["togetter.com/script.js"]
    }
  ]
}
